    {
      "parameters": {
        "jsCode": "// Normalize alert data from webhook\nconst items = $input.all();\nconst normalizedItems = [];\n\n// Process each input item\nfor (const item of items) {\n  const body = item.json.body || item.json;\n  \n  // Check if the body itself is an alert or contains alerts array\n  let alerts = [];\n  \n  if (Array.isArray(body.alerts)) {\n    // Body contains an alerts array\n    alerts = body.alerts;\n  } else if (Array.isArray(body)) {\n    // Body is an array of alerts\n    alerts = body;\n  } else if (body.source || body.message || body.title) {\n    // Body is a single alert object\n    alerts = [body];\n  } else {\n    // Fallback: treat the whole body as a single alert\n    alerts = [body];\n  }\n  \n  // Process each alert\n  for (const alert of alerts) {\n    normalizedItems.push({\n      json: {\n        source: alert.source || 'unknown',\n        title: alert.title || '',\n        message: alert.message || '',\n        credentials: alert.credentials || {},\n        telegramToken: alert.credentials?.telegram?.token || '',\n        telegramChatId: alert.credentials?.telegram?.chatId || '',\n        discordWebhookUrl: alert.credentials?.discord?.webhookUrl || '',\n        twitterConsumerKey: alert.credentials?.twitter?.consumerKey || '',\n        twitterConsumerSecret: alert.credentials?.twitter?.consumerSecret || '',\n        twitterAccessToken: alert.credentials?.twitter?.accessToken || '',\n        twitterAccessTokenSecret: alert.credentials?.twitter?.accessTokenSecret || ''\n      }\n    });\n  }\n}\n\nreturn normalizedItems;"
      },
